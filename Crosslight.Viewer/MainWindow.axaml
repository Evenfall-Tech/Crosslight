<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:graphv="clr-namespace:Crosslight.Viewer.Views.Graph"
		xmlns:graphvm="clr-namespace:Crosslight.Viewer.ViewModels.Graph"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Crosslight.Viewer.MainWindow"
        Title="Crosslight.Viewer">
	<Window.Resources>
		<graphvm:GraphViewerViewModel x:Key="vm"/>
	</Window.Resources>
	<graphv:GraphViewer DataContext="{StaticResource vm}"/>
</Window>
