<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Crosslight.GUI.Views.Explorers.ResultList"
        DataContext="{Binding ViewModel,RelativeSource={RelativeSource Self}}">
	<DockPanel LastChildFill="True">
		<TextBlock Text="Input" DockPanel.Dock="Top"/>
		<ListBox HorizontalAlignment="Stretch"
				 DockPanel.Dock="Top"
				 Name="resultListInput"
				 SelectionMode="Multiple,Toggle" Height="100">
		</ListBox>
		<TextBlock Text="Intermediate" DockPanel.Dock="Top"/>
		<ListBox HorizontalAlignment="Stretch"
				 DockPanel.Dock="Top"
				 Name="resultListInter"
				 SelectionMode="Multiple,Toggle" Height="100">
		</ListBox>
		<TextBlock Text="Output" DockPanel.Dock="Top"/>
		<ListBox HorizontalAlignment="Stretch"
				 DockPanel.Dock="Top"
				 Name="resultListOutput"
				 SelectionMode="Multiple,Toggle" Height="100">
		</ListBox>
	</DockPanel>
</UserControl>
